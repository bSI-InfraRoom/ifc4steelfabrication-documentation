        <TypePropertySingleValue>
          <DataType type="IfcIdentifier" />
        </TypePropertySingleValue>
      </PropertyType>
      <NameAliases>
        <NameAlias lang="en">Phase or Sequence ID</NameAlias>
      </NameAliases>
      <DefinitionAliases>
        <DefinitionAlias lang="en">Identifier for Phase or Sequence</DefinitionAlias>
      </DefinitionAliases>
    </PropertyDef>
    <PropertyDef ifdguid="d555713db9fc471eacede3b049867b84">
      <Name>SequenceLevel</Name>
      <Definition />
      <PropertyType>
        <TypePropertySingleValue>
          <DataType type="IfcIdentifier" />
        </TypePropertySingleValue>
      </PropertyType>
      <NameAliases>
        <NameAlias lang="en">Sequence level</NameAlias>
      </NameAliases>
      <DefinitionAliases>
        <DefinitionAlias lang="en">Sequence level</DefinitionAlias>
      </DefinitionAliases>
    </PropertyDef>
    <PropertyDef ifdguid="70c0369070264d10b534f16f048865a6">
      <Name>OnSiteBy</Name>
      <Definition />
      <PropertyType>
        <TypePropertySingleValue>
          <DataType type="IfcIdentifier" />
        </TypePropertySingleValue>
      </PropertyType>
      <NameAliases>
        <NameAlias lang="en">On Site By</NameAlias>
      </NameAliases>
      <DefinitionAliases>
        <DefinitionAlias lang="en">A property that allows for a sequence to have an “On site by” date.</DefinitionAlias>
      </DefinitionAliases>
