    <PropertyDef ifdguid="28456ed932984d22a367331b290364ea">
      <Name>NutType</Name>
      <Definition />
      <PropertyType>
        <TypePropertySingleValue>
          <DataType type="IfcPositiveLengthMeasure" />
        </TypePropertySingleValue>
      </PropertyType>
      <NameAliases>
        <NameAlias lang="en">Nut Type</NameAlias>
      </NameAliases>
